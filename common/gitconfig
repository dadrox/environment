[core]
	excludesfile = ~/.gitignore
[color]
	ui = auto
[color "status"]
	header = black bold
	branch = green
	nobranch = red reverse white
	added = green
	changed = yellow
	untracked = magenta
	deleted = red bold
[alias]
	git = !git
	st = status
	ci = commit
	co = checkout
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	amend = commit --amend
